estado(#uf, nome);
cidade(#id, &uf_estado, nome);
pessoa(#id, cpf, nome, sobrenome, data_nascimento, &id_cidade, bairro, cep, logradouro, numero_residencial, ddd, telefone, email);
deposito(#id, nome, descricao, &id_pessoa);
solicitante(#id, &id_pessoa, quantidade_dependentes);
parceiro(#id, cnpj, razao_social, nome_fantasia, cnae, &id_cidade, bairro, cep, logradouro, numero_residencial, ddd, telefone, email);
condutor(#id, &id_pessoa, cnh);
alimento(#id, nome, descricao);
item_higiene(#id, nome, descricao);
arrecadacao_alimento(#id, &id_alimento, data_validade, peso, $id_pessoa, &id_condutor, &id_deposito);
arrecadacao_item_higiene(#id, &id_item_higiene, data_validade, quantidade, $id_pessoa, &id_condutor, &id_deposito);
arrecadacao_financeira(#id, #id_pessoa, #id_parceiro, valor);
estoque_alimento(#id, &id_alimento, data_validade, peso, &id_deposito);
estoque_item_higiene(#id, &id_item_higiene, data_validade, quantidade, &id_deposito);
doacao_alimento(#id, &id_solicitante, &id_alimento, peso, &id_condutor, &id_estoque_alimento);
doacao_item_higiene(#id, &id_solicitante, &id_item_higiene, quantidade, &id_condutor, &id_estoque_item_higiene);
doacao_financeira(#id, &id_solicitante, valor);

